<%# `#routes`の先頭に登録されたrouteを追加する %>
<%= turbo_stream.prepend "routes-list", @route %>

<%# コピールートをdata-controller="copied_route"経由でマップを更新する %>
<%= turbo_stream.update "copied_route", partial: "copied_route", locals: { route: @route, org_route: @org_route } %>

<%# Flashを表示 %>
<%= turbo_stream_flash %>
